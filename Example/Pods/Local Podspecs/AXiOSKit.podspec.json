{
  "name": "AXiOSKit",
  "version": "2.0.0",
  "summary": "个人开发工具类",
  "description": "封装UIKit等个人开发工具类",
  "homepage": "https://github.com/axinger/axinger",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "axinger": "axingrun@outlook.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/axinger/AXiOSKit.git",
    "tag": "#{s.version}"
  },
  "source_files": "AXiOSKit/**/*.{h,m}",
  "exclude_files": "AXiOSKit/AXiOSKit/Info.plist",
  "public_header_files": "AXiOSKit/AXiOSKit/AXiOSKit.h",
  "resources": [
    "AXiOSKit/**/*.xib",
    "AXiOSKit/**/*.nib",
    "AXiOSKit/**/*.bundle"
  ],
  "resource_bundles": {
    "AXiOSKitMain": [
      "AXiOSKit/Assets/*.{xcassets,gif,json,wav}",
      "AXiOSKit/**/*.{strings}",
      "AXiOSKit/README/*.{md}"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.0"
    ],
    "SDWebImage": [

    ],
    "MJRefresh": [

    ],
    "NullSafe": [

    ],
    "MJExtension": [

    ],
    "MBProgressHUD": [

    ],
    "DZNEmptyDataSet": [

    ],
    "KVOController": [

    ],
    "Masonry": [

    ],
    "Aspects": [

    ],
    "ReactiveObjC": [

    ]
  }
}
