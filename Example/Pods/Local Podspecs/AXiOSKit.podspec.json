{
  "name": "AXiOSKit",
  "version": "2.1.0",
  "summary": "个人开发工具类",
  "description": "封装UIKit等个人开发工具类",
  "homepage": "https://github.com/axinger",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "axinger": "axingrun@outlook.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/axinger/AXiOSKit.git",
    "tag": "#{s.version}"
  },
  "public_header_files": "AXiOSKit/AXiOSKit/AXiOSKit.h",
  "source_files": "AXiOSKit/**/*.{h,m}",
  "resources": [
    "AXiOSKit/**/*.xib",
    "AXiOSKit/**/*.nib",
    "AXiOSKit/**/*.bundle"
  ],
  "resource_bundles": {
    "AXiOSKitMain": [
      "AXiOSKit/Assets/*.{xcassets,gif,json,wav}",
      "AXiOSKit/**/*.{strings}",
      "AXiOSKit/README/*.{md}"
    ]
  },
  "subspecs": [
    {
      "name": "Foundation",
      "public_header_files": "AXiOSKit/AXiOSKit/AXiOSFoundation.h",
      "source_files": "AXiOSKit/Foundation/**/*.{h,m}",
      "frameworks": "Foundation"
    },
    {
      "name": "Kit",
      "dependencies": {
        "AXiOSKit/Foundation": [

        ],
        "AFNetworking": [
          "~> 4.0.0"
        ],
        "SDWebImage": [

        ],
        "MJRefresh": [

        ],
        "NullSafe": [

        ],
        "MJExtension": [

        ],
        "MBProgressHUD": [

        ],
        "DZNEmptyDataSet": [

        ],
        "Masonry": [

        ],
        "Aspects": [

        ],
        "ReactiveObjC": [
          ">= 3.0.0"
        ],
        "AXViewControllerTransitioning": [

        ],
        "lottie-ios": [
          "~>2.5.3"
        ]
      },
      "frameworks": [
        "Foundation",
        "UIKit"
      ]
    }
  ]
}
